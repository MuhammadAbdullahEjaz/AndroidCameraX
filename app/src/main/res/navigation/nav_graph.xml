<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/camera">

    <fragment
        android:id="@+id/camera"
        android:name="com.example.camera.Camera"
        android:label="Camera" >
        <action
            android:id="@+id/action_camera_to_imagePicker"
            app:destination="@id/imagePicker" />
    </fragment>
    <fragment
        android:id="@+id/imagePicker"
        android:name="com.example.camera.ImagePicker"
        android:label="fragment_image_picker"
        tools:layout="@layout/fragment_image_picker" />
</navigation>